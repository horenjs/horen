{
  "name": "main",
  "version": "0.0.1",
  "description": "electron template with react and typescript",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "start": "pnpm build && pnpm dev",
    "dev": "cross-env NODE_ENV=development electron ../../dist/main.js",
    "prod": "cross-env NODE_ENV=production electron ../../dist/main.js",
    "build": "rollup -c rollup.config.mjs"
  },
  "keywords": [
    "electron",
    "template",
    "react",
    "typescript"
  ],
  "author": "Kevin Jobs <me@kevinjobs.com>",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/node": "^18.15.11",
    "cross-env": "^7.0.3",
    "electron": "latest",
    "music-metadata": "^7.14.0",
    "rollup": "^4.9.6",
    "tslib": "^2.6.2",
    "winston": "^3.11.0"
  },
  "dependencies": {
    "lowdb": "^7.0.1"
  }
}
